TARGETS = [expand('sim_summary_snp{snpnum2}.csv',snpnum2=range(1,3)),]


rule DONE:
	input: TARGETS

rule sims_iter_snp:
	input: 'lungFx10kLong.csv.gz'
	output: 'sim{iter}_snp{snpnum}_lmm.csv'
	script: 'sims_snakemake.R'

rule compile_results:	
    input: expand('sim{iter}_snp{snpnum}_lmm.csv',iter=range(1,1001),snpnum=range(1,3))
    output: 'sims_lmm_compiled.csv'
    script: 'sims_compile.R'
    
rule summarise_results:
    input: 'sims_lmm_compiled.csv'
    output: 'sim_summary_snp{snpnum2}.csv'
    script: 'summarise_sims.R'
    